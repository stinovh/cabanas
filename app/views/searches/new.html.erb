<%= simple_form_for @search do |f| %>
  <%= f.label :price_night %>
  <%= f.number_field :price_night %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :capacity %>
  <%= f.number_field :capacity %>
  <%= f.date_field :start_date %>
  <%= f.date_field :end_date %>
  <%= f.button :submit, "Search" %>
<% end %>
